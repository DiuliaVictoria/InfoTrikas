 answer.innerHTML = ""

            for (var posicao = 0; posicao < ticket_list.length; posicao++) {
                if (qnt_ticket > 5) {
                    alert("Voce pode comprar no Max√≠mo 5 ingressos")
                }

                else {
                    answer.innerHTML = `${name} <br>
        Sua compra de ${ticket_list[posicao]} 
        Ingressos foi realizada com Sucesso! <br> <br>
        Valor Total ${qnt_ticket * 150} R$`
                }
            }